    schema:identifier "TW-78" ;
    schema:isPartOf <http://localhost:8000/cardset/TW> ;
    schema:quotation "<p><i>“… he saw the goblins: goblins in full armour with drawn swords… They were aroused, alert, ready for anything.” —</i><b><i>Hob</i></b></p>"@en,
        "<p><i>“… de pronto vio a los trasgos; trasgos armados de pies a cabeza con las espadas desenvainadas… Estaban preparados, atentos, dispuestos a cualquier cosa.” —</i><b><i>Hob</i></b></p>"@es,
        "<p><i>[...] il vit les gobelins : des gobelins en armure avec des épées tirées [...] Ils étaient attentifs, alertes, prêts à tout. <b>—Hob</i></p>"@fr .

<http://localhost:8000/card/TW-79> a schema:CreativeWork ;
    rdfs:label "Taschendieb"@de,
        "Pick-pocket"@en,
