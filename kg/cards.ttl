    schema:isPartOf <http://localhost:8000/cardset/TW> ;
    schema:quotation "<p><i>“The rider was robed all in black, and black was his lofty helm… The Lieutenant of the Tower of Barad-dûr…” —</i><b><i>LotRV</i></b></p>"@en,
