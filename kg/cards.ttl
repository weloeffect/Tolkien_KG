<http://localhost:8000/card/LE-171> a schema:CreativeWork ;
    rdfs:label "Abgelenkter Hieb"@de,
        "Blow Turned"@en,
        "Golpe Desviado"@es,
        "Coup dévié"@fr,
        "Blow Turned"@nl ;
