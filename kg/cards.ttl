    schema:isPartOf <http://localhost:8000/cardset/TD> ;
    schema:quotation "<p><i>“The winds of wrath came driving him, and blindly in the foam he fled from west to east and errandless…”</i><b><i>  </i></b><i>—</i><b><i>LotRII</i></b></p>"@en,
        "<p><i>“Los vientos de cólera se alzaron arrastrándolo y a ciegas escapó de la espuma del este hacia el oeste…”</i> <i>—</i><b><i>SdlA I</i></b></p>"@es,
