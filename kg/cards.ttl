<http://localhost:8000/card/TD-90> a schema:CreativeWork ;
    rdfs:label "Brand"@de,
        "Brand"@en,
        "Brand"@es,
        "Brand"@fr,
        "ブランド"@ja,
        "Brand"@nl ;
    schema:about <http://localhost:8000/resource/Brand> ;
    schema:description "<p><b><i>Unique.</i></b> +2 direct influence against <i>Men of Dale</i> faction.</p>"@en,
        "<p><b><i>Único.</i></b> +2 a su influencia directa contra la facción de los <i>Hombres de Valle</i>.</p>"@es,
        "<p><i><b>Unique. </b></i>+2 à l’influence directe sur la faction des <i>Hommes du Val</i>.</p>"@fr ;
    schema:identifier "TD-90" ;
    schema:isPartOf <http://localhost:8000/cardset/TD> ;
