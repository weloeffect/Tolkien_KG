<http://localhost:8000/card/LE-172> a schema:CreativeWork ;
    rdfs:label "Gewagter Hieb"@de,
        "Bold Thrust"@en,
        "Embestida Violenta"@es,
        "Assaut audacieux"@fr,
        "Bold Thrust"@nl ;
    schema:description "<p><i>Warrior only.</i> Warrior receives +3 to prowess and -1 to body against one strike.</p>"@en,
        "<p><i>Solo guerreros.</i> El guerrero recibe +3 a su poder y</p><p>-1 a la resistencia contra un golpe.</p>"@es,
