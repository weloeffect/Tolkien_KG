    schema:identifier "TW-144" ;
    schema:isPartOf <http://localhost:8000/cardset/TW> ;
    schema:quotation "<p><i>“Then said Elrohir: ‘From the North we came with this purpose, and from Elrond our father we brought this very counsel. We will not turn back.’”</i> <i>—</i><b><i>LotRV</i></b></p>"@en,
