    schema:identifier "BA-93" ;
    schema:isPartOf <http://localhost:8000/cardset/BA> ;
    schema:quotation "<p><i>“The Mines of Moria were vast and intricate beyond the imagination…” —</i><b><i>LotRII</i></b></p>"@en,
